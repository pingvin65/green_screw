package com.pink.screw.java.payroll;

public class SupplierNotFoundException extends RuntimeException {

	/**
	 * 
	 */
	private static final long serialVersionUID = 2813601500284143571L;

	public SupplierNotFoundException(Long id) {
		
		super("{\"mesage\": \"Could not find Products " + id + "\"");
	}
}
